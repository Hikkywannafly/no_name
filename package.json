{
  "name": "minimil",
  "version": "0.1.0",
  "private": true,
  "description": "A sleek and minimal Next.js starter template, optimized for performance and rapid development.",
  "author": "hikkywannafly(aleckurai)",
  "keywords": [
    "nextjs",
    "react",
    "boilerplate",
    "template",
    "starter",
    "tailwindcss",
    "minimal",
    "sleek",
    "clean",
    "lightweight",
    "fast",
    "modern",
    "optimized",
    "developer-friendly"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "preview": "next build && next start",
    "lint": "biome lint --write .",
    "lint:fix": "biome lint --write . --unsafe",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "commit": "git cz",
    "prepare": "husky install",
    "build:prepare": "pnpm run typecheck && pnpm run lint && pnpm run format && pnpm run check && pnpm run build",
    "test:parser": "node scripts/test-parser.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit/",
    "test:integration": "jest tests/integration/",
    "test:components": "jest --config jest.config.components.js",
    "test:setup": "bash scripts/setup-tests.sh"
  },
  "dependencies": {
    "@biomejs/biome": "^1.9.4",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.2",
    "@serwist/next": "^9.0.13",
    "@tiptap/extension-image": "^2.25.0",
    "@tiptap/extension-link": "^2.25.0",
    "@tiptap/extension-mention": "^2.25.1",
    "@tiptap/extension-placeholder": "^2.25.1",
    "@tiptap/extension-underline": "^2.25.0",
    "@tiptap/pm": "^2.25.0",
    "@tiptap/react": "^2.25.0",
    "@tiptap/starter-kit": "^2.25.0",
    "@vercel/analytics": "^1.5.0",
    "axios": "^1.10.0",
    "axios-cookiejar-support": "^6.0.4",
    "cheerio": "^1.1.0",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.6.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.485.0",
    "next": "15.3.2",
    "next-themes": "^0.4.6",
    "nextjs-toploader": "^3.8.16",
    "node-fetch": "^3.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-loading-skeleton": "^3.5.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "slugify": "^1.6.6",
    "swr": "^2.3.3",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "tough-cookie": "^5.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@tailwindcss/postcss": "^4.1.7",
    "@testing-library/jest-dom": "latest",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^15.4.3",
    "serwist": "^9.0.12",
    "tailwindcss": "^4.1.7",
    "ts-jest": "^29.1.0",
    "typescript": "^5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "npx --no -- commitlint --edit $1",
      "pre-commit": "npx --no -- lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": ["biome lint --apply ."],
    "*.{json,css,html,md}": ["biome format --write ."]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
